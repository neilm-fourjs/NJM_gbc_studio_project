{
  "palette-primary": "$mt-blue-900",
  "theme-separator-color": "$mt-blue-900",
  "theme-primary-faded-background-color": "$mt-blue-200",
  "theme-disabled-background-color": "$palette-disabled-600",
  "theme-message-background-color": "$theme-secondary-color",
  "theme-font-size-ratio": 1.2,
  "theme-margin-ratio": 1,
  "theme-field-height-ratio": 1,
  "theme-radiobutton-size": "24px",
  "theme-checkbox-size": "24px",
  "mt-field-margin-ratio": 0,
  "mt-button-disabled-background-color": "$theme-secondary-faded-color",
  "mt-button-disabled-color": "$theme-primary-color",
  "mt-progress-height": "8px",
  "mt-field-border-width-top": "2px",
  "mt-field-border-width-right": "2px",
  "mt-field-border-width-bottom": "2px",
  "mt-field-border-width-left": "2px",
  "mt-field-focus-border-width-top": "3px",
  "mt-field-focus-border-width-right": "3px",
  "mt-field-focus-border-width-bottom": "3px",
  "mt-field-focus-border-width-left": "3px",
  "mt-field-border-color": "$theme-secondary-color",
  "mt-field-focus-border-color": "$gbc-genero-green",
  "gbc-SliderWidget-color": "$gbc-genero-green",
  "gbc-SliderWidget-field-height-ratio": 1.4,
  "gbc-SpinEditWidget-arrows-size": "0.7em",
  "gbc-ComboBoxWidget-toggle-size": "1.2em",
  "gbc-ComboBoxWidget-padding": "1px",
  "gbc-ApplicationHostMenuWidget-runtime-processing-opacity": 1,
  "gbc-CheckBoxWidget-checked-color": "$gbc-genero-green",
  "gbc-DropDownWidget-item-current-color": "$theme-secondary-color",
  "gbc-DropDownWidget-item-current-background-color": "$theme-primary-faded-background-color"
}
